prefix=/opt/homebrew/Cellar/fontconfig/2.17.1
includedir=${prefix}/include
libdir=${prefix}/lib

sysconfdir=/opt/homebrew/etc
localstatedir=/opt/homebrew/var
confdir=/opt/homebrew/etc/fonts
cachedir=/opt/homebrew/var/cache/fontconfig

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.17.1
Requires.private: freetype2 >=  21.0.15, expat
Libs: -L${libdir} -lfontconfig
Libs.private: -lintl -lm
Cflags: -I${includedir}
